@using Gusker.Models

@model PageViewModel<DefaultPageViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>@Model.Data.Title</h2>

@Html.Kentico().EditableArea("center")

@section styles {
    @if (Context.Kentico().PageBuilder().EditMode)
    {
        @Html.Kentico().PageBuilderStyles()
    }
}

@section scripts {
    @if (Context.Kentico().PageBuilder().EditMode)
    {
        @Html.Kentico().PageBuilderScripts()
    }
}
